{
  "compilerOptions": {
    "baseUrl": "./",
    "paths": {
      "src/*": ["./src/"]
    },
    "module": "commonjs",
    "target": "esnext",
    "jsx": "preserve",
    "lib": ["dom", "esnext", "ESNext.Intl", "ES2017.Intl", "ES2018.Intl"],
    "strict": true,
    "noEmit": true,
    "isolatedModules": true,
    "esModuleInterop": true,
    "noUnusedLocals": false
  },
  "exclude": ["node_modules", "public", ".cache", "gatsby-*.js"]
}
